{
  "name": "alynt/alynt-certificate-generator",
  "description": "Generate PDF certificates from templates with variables and automation.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.4",
    "tecnickcom/tcpdf": "^6.6",
    "woocommerce/action-scheduler": "^3.7"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.10",
    "wp-coding-standards/wpcs": "^3.0",
    "phpunit/phpunit": "^9.6",
    "brain/monkey": "^2.6"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
